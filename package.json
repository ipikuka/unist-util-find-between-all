{
  "name": "unist-util-find-between-all",
  "version": "1.0.4",
  "description": "unist utility to find the nodes between two nodes",
  "license": "MIT",
  "keywords": [
    "unist",
    "unist-util",
    "util",
    "utility",
    "tree",
    "node",
    "find",
    "between"
  ],
  "repository": "ipikuka/unist-util-find-between-all",
  "bugs": "https://github.com/ipikuka/unist-util-find-between-all/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "ipikuka <talatkuyuk@gmail.com>",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "lib/",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@types/unist": "^3.0.2",
    "unist-util-is": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "c8": "^8.0.1",
    "mdast-util-from-markdown": "^2.0.0",
    "prettier": "^3.1.1",
    "remark-cli": "^12.0.0",
    "remark-preset-wooorm": "^9.0.0",
    "type-coverage": "^2.27.1",
    "typescript": "^5.3.3",
    "xo": "^0.56.0"
  },
  "scripts": {
    "prepack": "npm run build && npm run format",
    "build": "tsc --build --clean && tsc --build && type-coverage",
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node --conditions development test.js",
    "test-coverage": "c8 --check-coverage --100 --reporter lcov npm run test-api",
    "test": "npm run build && npm run format && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "strict": true
  }
}
