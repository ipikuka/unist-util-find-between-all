{
  "name": "unist-util-find-between-all",
  "version": "1.1.1",
  "description": "unist utility to find the nodes between two nodes",
  "license": "MIT",
  "keywords": [
    "unist",
    "unist-util",
    "util",
    "utility",
    "tree",
    "node",
    "find",
    "between"
  ],
  "repository": "ipikuka/unist-util-find-between-all",
  "bugs": "https://github.com/ipikuka/unist-util-find-between-all/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "ipikuka <talatkuyuk@gmail.com>",
  "contributors": [
    "Talat Küyük <talatkuyuk@gmail.com> (https://ipikuka.com)",
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "files": [
    "dist/",
    "license",
    "README.md"
  ],
  "dependencies": {
    "@types/unist": "^3.0.3",
    "unist-util-is": "^6.0.0"
  },
  "devDependencies": {
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.10.0",
    "c8": "^10.1.3",
    "mdast-util-from-markdown": "^2.0.2",
    "prettier": "^3.6.2",
    "remark-cli": "^12.0.1",
    "remark-preset-wooorm": "^11.0.0",
    "rimraf": "^6.1.0",
    "tsd": "^0.33.0",
    "type-coverage": "^2.29.7",
    "typescript": "^5.9.3",
    "xo": "^1.2.3"
  },
  "scripts": {
    "prepack": "npm run build && npm run format",
    "build": "rimraf dist && tsc --build && tsd && type-coverage",
    "format": "prettier . -w --log-level warn && xo --fix",
    "test-api": "node --conditions development tests/test.js",
    "test-coverage": "c8 --100 --reporter lcov npm run test-api",
    "test": "npm run build && npm run format && npm run test-coverage"
  },
  "prettier": {
    "bracketSpacing": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-wooorm"
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "ignoreCatch": true,
    "#": "needed `any`s",
    "ignoreFiles": [
      "lib/index.d.ts"
    ],
    "strict": true
  },
  "xo": {
    "prettier": true,
    "space": true
  },
  "tsd": {
    "directory": "dist"
  }
}
